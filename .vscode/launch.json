{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "TypeScript",
            "program": "${file}",
            "request": "launch",
            "preLaunchTask": "npm: build",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "cwd": "${fileDirname}"
        },
        {
            "name": "Python",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${fileDirname}"
        }
    ]
}